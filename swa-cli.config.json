{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "azure-functions-open-ai-demo": {
      "appLocation": "app/frontend",
      "apiLocation": "app/backend",
      "outputLocation": "dist",
      "apiLanguage": "dotnetisolated",
